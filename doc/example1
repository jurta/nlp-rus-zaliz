wf(w(абак,3),с(р,м),w(абаков,3)).
wf(w(медленный,2),а(р,ж,е),w(медленной,2)).
wf(w(этот,1),м(и,ж,е),w(эта,1)).
wf(w(делать,2),г(действ,наст,2),w(делаешь,2)).

================================================================
w(посол,m(4,с(м,о),с("м1*b"),[c('дипломат')])).
w(посол,m(4,с(м,н),с("м1a"),[c('засол')])).

wf(w(посол,4),'дипломат',с(и,м),w(послы,5)).
wf(w(посол,4),'дипломат',с(р,м),w(послов,5)).
wf(w(посол,4),'засол',с(и,м),w(посолы,4)).
wf(w(посол,4),'засол',с(р,м),w(посолов,4)).

