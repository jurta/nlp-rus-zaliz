wf_main(W,WF).


w(абак,m(3,с(м,н),с("м3а"),[w(абака,m(3))])).
wf_main("абак",3,с(н,"м3а"),X,A).

w(боты,m(2,с(м,н,мн),[с("м1а"),с("м1а2")])).
wf_main("боты",с(м,н,мн),2,[с("м1а"),с("м1а2")],X,A).

w(домашние,m(4,п(мн,о),а("2a"))). % с(мн,о)???
wf_main("домашние",п(мн,о),4,а("2a"),X,A).
wf_main(п(мн,о),а("2a"),w(X,A),w("домашних",4)).

