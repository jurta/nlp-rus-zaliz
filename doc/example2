w(акант,m(3,с(м,н),с("м1а"),[w(аканф,m(3,[c('устар.')]))])).

аканф 3 м 1а [//ак┬нт (_см._)] 
w(аканф,m(3,с(м,н),с("м1а"),[w(акант,m(3,[c('см.')]))])).

акушер 5,5 мо 1а [//акуш┼р мо 1а] accoucheur obstetrician
w(акушер,m(a(e(5)),с(м,о),с("м1а"),[w(акушер,m(5))])).
w(акушер,m(5,      с(м,о),с("м1а"),[])).

