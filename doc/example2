w(абака,m(3,с(ж,н),с("ж3а"),[w(абак,m(3)),c('архит.')])).

авар 3 мо 1а["2"] 
w(авар,m(3,с(м,о),[с("м1а"),с("м1а2")],[])).

авось 3 част.; вводн. % на ав▒сь 
w(а,[m(3,част,0,[]),m(3,вводн,0,[%('на ав▒сь')])]).

